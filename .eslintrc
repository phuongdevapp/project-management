{
  "extends": "airbnb",
  "rules": {
    "comma-dangle": [
      "error",
      "never"
    ],
    "react/prefer-stateless-function": 0,
    "import/prefer-default-export": 0,
    "react/jsx-filename-extension": 0,
    "no-use-before-define": 0,
    "react/prop-types": 0,
    "linebreak-style": 0,
    "import/extensions": 0,
    "class-methods-use-this": 0,
    "no-useless-escape": 0,
    "no-underscore-dangle": 0,
    "react/sort-comp": 0,
    "max-len": 0,
    "global-require": 0
  }
}